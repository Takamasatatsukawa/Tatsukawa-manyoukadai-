<h1>ログインページ</h1>
<%= form_with(scope: :session, url: login_path, local: true) do |form| %>
  <div>
    <%= form.label :email, "メールアドレス" %>
    <%= form.email_field :email, id: 'email' %>
  </div>

  <div>
    <%= form.label :password, "パスワード" %>
    <%= form.password_field :password, id: 'password' %>
  </div>

  <div>
    <%= form.submit "ログイン", id: 'create-session' %>
  </div>
<% end %>
